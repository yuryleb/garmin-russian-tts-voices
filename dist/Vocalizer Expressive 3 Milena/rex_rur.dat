
[header]
language = RUR
charset = utf-8

[data]
# Correct pronouncation of 80 number
/\b80\b/ --> "/+'2vos%j.\$m%j.d%j\$.s%jo-t/+"

# Correct time pronouncation greater 21 hour
/\b21:(\d\d)\b/ --> " 20 01:$1 "
/\b2([2-4]:\d\d)\b/ --> " 20 0$1 "

# Correct accent for some words combinations
/([иы]е) полосы/ --> "$1 /+'po.lo-.si0/+"
/([мт]и) вечера/ --> "$1 /+'v%je.t&S%j$.ro-/+"
